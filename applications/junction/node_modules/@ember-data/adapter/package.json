{
  "name": "@ember-data/adapter",
  "version": "5.3.13",
  "description": "Provides Legacy JSON:API and REST Implementations of the Adapter Interface for use with @ember-data/store",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:emberjs/data.git",
    "directory": "packages/adapter"
  },
  "license": "MIT",
  "author": "",
  "directories": {},
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "files": [
    "unstable-preview-types",
    "blueprints",
    "addon-main.cjs",
    "dist",
    "README.md",
    "LICENSE.md",
    "logos"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./blueprints/*": {
      "default": "./blueprints/*.js"
    },
    "./*": {
      "default": "./dist/*.js"
    },
    "./unstable-preview-types": {
      "types": "./unstable-preview-types/index.d.ts"
    }
  },
  "peerDependencies": {
    "ember-source": "3.28.12 || ^4.0.4 || ^5.0.0 || ^6.0.0",
    "@ember-data/legacy-compat": "5.3.13",
    "@ember-data/store": "5.3.13",
    "@ember-data/request-utils": "5.3.13",
    "@warp-drive/core-types": "0.0.3"
  },
  "dependencies": {
    "@embroider/macros": "^1.16.12",
    "ember-cli-test-info": "^1.0.0",
    "ember-cli-string-utils": "^1.1.0",
    "ember-cli-path-utils": "^1.0.0",
    "@ember/edition-utils": "1.2.0",
    "@warp-drive/build-config": "0.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-typescript": "^7.27.0",
    "@babel/preset-typescript": "^7.27.0",
    "@ember-data/graph": "5.3.13",
    "@ember-data/json-api": "5.3.13",
    "@ember-data/legacy-compat": "5.3.13",
    "@ember-data/request": "5.3.13",
    "@ember-data/request-utils": "5.3.13",
    "@ember-data/store": "5.3.13",
    "@ember-data/tracking": "5.3.13",
    "@ember/test-waiters": "^4.1.0",
    "@glimmer/component": "^2.0.0",
    "decorator-transforms": "^2.3.0",
    "@types/jquery": "^3.5.32",
    "@warp-drive/core-types": "0.0.3",
    "@warp-drive/internal-config": "5.3.13",
    "ember-source": "~6.3.0",
    "typescript": "^5.8.3",
    "vite": "^5.4.15"
  },
  "engines": {
    "node": ">= 18.20.8"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "typesVersions": {
    "*": {
      "unstable-preview-types": [
        "./unstable-preview-types"
      ]
    }
  },
  "scripts": {
    "lint": "eslint . --quiet --cache --cache-strategy=content",
    "build:pkg": "vite build;",
    "sync": "echo \"syncing\"",
    "start": "vite"
  }
}