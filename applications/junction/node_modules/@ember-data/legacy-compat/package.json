{
  "name": "@ember-data/legacy-compat",
  "description": "Compatibility Shims for Older EmberData",
  "version": "5.3.13",
  "license": "MIT",
  "author": "Chris Thoburn <runspired@users.noreply.github.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:emberjs/data.git",
    "directory": "packages/legacy-compat"
  },
  "homepage": "https://github.com/emberjs/data",
  "bugs": "https://github.com/emberjs/data/issues",
  "engines": {
    "node": ">= 18.20.8"
  },
  "keywords": [
    "ember-addon"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "files": [
    "unstable-preview-types",
    "addon-main.cjs",
    "dist",
    "README.md",
    "LICENSE.md",
    "logos"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./*": {
      "default": "./dist/*.js"
    },
    "./unstable-preview-types": {
      "types": "./unstable-preview-types/index.d.ts"
    }
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "peerDependenciesMeta": {
    "@ember-data/graph": {
      "optional": true
    },
    "@ember-data/json-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@embroider/macros": "^1.16.12",
    "@warp-drive/build-config": "0.0.3"
  },
  "peerDependencies": {
    "ember-source": "3.28.12 || ^4.0.4 || ^5.0.0 || ^6.0.0",
    "@ember-data/graph": "5.3.13",
    "@ember-data/json-api": "5.3.13",
    "@ember-data/request": "5.3.13",
    "@ember-data/request-utils": "5.3.13",
    "@ember-data/store": "5.3.13",
    "@ember/test-waiters": "^3.1.0 || >= 4.0.0",
    "@warp-drive/core-types": "0.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-typescript": "^7.27.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@ember-data/graph": "5.3.13",
    "@ember-data/json-api": "5.3.13",
    "@ember-data/request": "5.3.13",
    "@ember-data/request-utils": "5.3.13",
    "@ember-data/store": "5.3.13",
    "@ember-data/tracking": "5.3.13",
    "@ember/test-waiters": "^4.1.0",
    "@glimmer/component": "^2.0.0",
    "@warp-drive/core-types": "0.0.3",
    "@warp-drive/internal-config": "5.3.13",
    "ember-source": "~6.3.0",
    "typescript": "^5.8.3",
    "vite": "^5.4.15"
  },
  "ember": {
    "edition": "octane"
  },
  "typesVersions": {
    "*": {
      "unstable-preview-types": [
        "./unstable-preview-types"
      ]
    }
  },
  "scripts": {
    "lint": "eslint . --quiet --cache --cache-strategy=content",
    "build:pkg": "vite build;",
    "sync": "echo \"syncing\"",
    "start": "vite"
  }
}